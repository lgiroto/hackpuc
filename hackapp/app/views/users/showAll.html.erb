<%= render partial: "topbar/default", locals: {active_button: "none"} %>

<div class="container">
		<div class="row">
			<div class="col-md-2">
				<%= link_to 'Voltar', home_index_path, {:class => "btn btn-primary"} %>
			</div>
		</div>
		<div class="row">
			<div class="col-md-12">
				<table class="table table-striped table-bordered ">
					<tr>
						<th>Nome</th>
						<th>Email</th> 
						<th>Senha Encriptografada</th>
					</tr>
					<% @allusers.each do |user| %>
						<tr>
							<td><%= user.name %></td>
							<td><%= user.email %></td>
							<td><%= user.password_digest %></td>
						<tr/>
					<% end %>
				</table>
			</div>
		</div>
</div>

